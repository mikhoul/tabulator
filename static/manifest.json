{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Tabulator Mikhoul Fork",
  "author": "Mkhoul based on Ammar Khaku work",
  "version": "1.0.2",
  "description": "Help to manage your open tabs",
  "browser_action": {
            "default_icon": "images/icon-128.png"
        },
 
   "background": {
    "scripts": ["bundles/background.js","showHTML.js"],
	"persistent": true
  },
  
   "icons": {
            "128": "images/icon-128.png"
        },

  "permissions": [
    "tabs",
    "<all_urls>"
  ],

  "minimum_chrome_version": "50",
  
  "offline_enabled": true
  
}
